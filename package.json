{
  "name": "ah-frontend-iroquois",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.5.7",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "prop-types": "latest",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-materialize": "^2.4.6",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-router-redux": "^4.0.8",
    "react-social-icons": "^3.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode development --open --hot",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/App.js",
      "!src/redux-js/stores/index.js",
      "!src/views/Home.js",
      "!src/views/Login.js",
      "!src/views/Navigation.js",
      "!src/redux-js/reducers/index.js"
    ],
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "latest",
    "jest": "^23.6.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
