{
  "name": "ah-frontend-iroquois",
  "version": "0.1.0",
  "private": true,
  "heroku config": "set NPM_CONFIG_PRODUCTION=false",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^11.1.1",
    "@ckeditor/ckeditor5-build-decoupled-document": "^11.1.1",
    "@ckeditor/ckeditor5-build-inline": "^11.1.1",
    "@ckeditor/ckeditor5-react": "^1.0.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.5.7",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "history": "latest",
    "jimp": "^0.5.4",
    "jquery": "^3.3.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "prop-types": "latest",
    "react": "^16.5.2",
    "react-chips": "^0.8.0",
    "react-dom": "^16.5.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^3.2.1",
    "react-materialize": "^2.4.7",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.5",
    "react-social-icons": "^3.0.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "responsive-loader": "^1.1.0"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode development --open --hot",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "postinstall": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/redux-js/stores/index.js",
      "!src/App.js",
      "!src/redux-js/stores/index.js",
      "!src/views/Home.js",
      "!src/views/Login.js",
      "!src/views/Navigation.js",
      "!src/redux-js/reducers/index.js",
      "!src/static/js/material-dialog.js"
    ],
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/setupTests.js"
    ],
    "globals": {
      "onbeforeunload": true
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.0.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5"
  }
}
